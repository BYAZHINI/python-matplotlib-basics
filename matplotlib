# another program
import matplotlib.pyplot as plt
m=["Jammu","Srinagar","Anantnag","Baramula","Kupwara","Badwagam",
   "Rajouri","Kathuda","Pulwama","Udampur","Punch","Kulgam","Doda",
   "Bandipore","Samba","Reasi","Ganderbal","Ramban","Shupiyan",
   "Kishtwar","Kargil","Leh(Ladakh)"]
q=[50.00,1.40,73.77,81.90,87.97,87.01,91.86,85.45,85.64,80.50,
   91.90,81.01,92.03,83.34,83.19,91.42,84.19,95.84,93.85,93.56,
   88.40,65.79]
plt.bar(m,q,width=0.5,color="green",edgecolor="black")
plt.xlabel("DISTRICT")
plt.ylabel("RURAL")
plt.title("DISTRICT vs RURAL")
plt.xticks(m,[str(i)for i in a],rotation=90)
plt.tick_params(axis='x',which='major',labelsize=8)
plt.show()
